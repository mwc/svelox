{
	"name": "svelox",
	"version": "0.0.3",
	"description": "Make Svelte's reactivity compatible with object APIs.",
	"keywords": [
		"svelox",
		"svelte",
		"Array",
		"push",
		"splice"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mwc/svelox.git"
	},
	"bugs": {
		"url": "https://github.com/mwc/svelox/issues"
	},
	"homepage": "https://github.com/mwc/svelox",
	"author": {
		"name": "mwc",
		"url": "https://github.com/mwc"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package && node build",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0",
		"vitest": "^0.25.3"
	},
	"license": "MIT",
	"type": "module"
}